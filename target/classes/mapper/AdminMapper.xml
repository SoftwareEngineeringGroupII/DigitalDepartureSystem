<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--namespace=绑定一个对应的mapper接口-->
<mapper namespace="com.digitaldeparturesystem.mapper.AdminMapper">

    <!--select查询语句-->

    <!--对象中的属性可以换直接取出来-->
    <insert id="addClerk" parameterType="com.digitaldeparturesystem.pojo.Clerk">
        insert into clerk(clerkID,clerkName,clerkPwd,department,clerkPower,clerkAccount,clerkPhoto,clerkStatus)
        values(#{clerkID},#{clerkName},#{clerkPwd},#{department},#{clerkPower},#{clerkAccount},#{clerkPhoto},#{clerkStatus});
    </insert>

</mapper>