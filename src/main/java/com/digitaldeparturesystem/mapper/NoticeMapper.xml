<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.digitaldeparturesystem.mapper.NoticeMapper">

    <!--添加公告-->
    <insert id="save" parameterType="com.digitaldeparturesystem.pojo.Notice">
        insert into notice(noticeID,content,title,publisherId,publishTime
        ,checkStatus,remark,noticeType,isTop) values(#{noticeID},#{content},#{title},#{publisherId},#{publishTime}
        ,#{checkStatus},#{remark},#{noticeType},#{isTop});
    </insert>



</mapper>