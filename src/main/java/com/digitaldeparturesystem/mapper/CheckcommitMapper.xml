<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<!--namespace=绑定一个对应的mapper接口-->
<mapper namespace="com.digitaldeparturesystem.mapper.CheckcommitMapper">

    <!--select查询语句-->

    <!--对象中的属性可以换直接取出来-->
    <insert id="addClerk" parameterType="java.lang.String">
        insert into checkcommit (stuId,reason,type)
        values(#{stuId},#{reason},#{type});
    </insert>
    <update id="updateCheckcommit" parameterType="java.lang.String">
        update checkcommit set
        `stuId` = #{stuId},
        `reason` = #{reason},
        `type` = #{type}
        where checkId = #{checkId};
    </update>

</mapper>